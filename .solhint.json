{
    "extends": "solhint:recommended",
    "plugins": ["prettier"],
    "rules": {
        "func-order": "off",
        "no-empty-blocks": "off",
        "no-unused-vars": "off",
        "compiler-version": "off",
        "not-rely-on-time": "off",
        "reason-string": "off",
        "prettier/prettier": "error"
    }
}
